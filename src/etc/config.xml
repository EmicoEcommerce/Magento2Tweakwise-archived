<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <tweakwise>
            <general>
                <authentication_key/>
                <server_url>http://navigator-group1.tweakwise.com/</server_url>
                <log>0</log>
                <log_developer>1</log_developer>
                <timeout>10</timeout>
            </general>
            <layered>
                <enabled>0</enabled>
                <category_links>1</category_links>
                <hide_single_option>1</hide_single_option>
                <default_link_renderer>1</default_link_renderer>
            </layered>
            <autocomplete>
                <enabled>0</enabled>
                <show_products>1</show_products>
                <show_categories>1</show_categories>
                <in_current_category>0</in_current_category>
                <max_results>50</max_results>
            </autocomplete>
            <search>
                <enabled>0</enabled>
                <template/>
            </search>
        </tweakwise>
    </default>
</config>
