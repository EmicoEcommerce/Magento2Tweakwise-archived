{
    "name": "emico/tweakwise",
    "license": "OSL-3.0",
    "description": "Magento 2 module for Tweakwise integration",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "emico/tweakwise-export": ">=4.0",
        "guzzlehttp/guzzle": ">=6.0",
        "magento/framework": "*"
    },
    "require-dev": {
        "malukenho/docheader": "~0.1",
        "phpro/grumphp": "1.2.0"
    },
    "type": "magento2-module",
    "scripts": {
        "docheader": "docheader check ."
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Emico\\Tweakwise\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ]
}
