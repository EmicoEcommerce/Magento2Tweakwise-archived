{
    "name": "emico/tweakwise",
    "license": "OSL-3.0",
    "description": "Magento 2 module for Tweakwise integration",
    "require": {
        "php": "~5.6|~7.0",
        "magento/module-catalog-search": "100.1.*",
        "emico/tweakwise-export": "~1.0-dev"
    },
    "require-dev": {
        "malukenho/docheader": "~0.1",
        "phpro/grumphp": "^0.11.1"
    },
    "type": "magento2-module",
    "version": "1.0.0",
    "scripts": {
        "docheader": "docheader check ./src"
    },
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Emico\\Tweakwise\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/EmicoEcommerce/Magento2TweakwiseExport.git"
        }
    ]
}
